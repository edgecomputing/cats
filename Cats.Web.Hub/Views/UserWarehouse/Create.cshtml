@model Cats.Models.Hub.UserWarehouse
@using Cats.Web.Hub.Helpers
@{
    ViewBag.Title = Html.Translate("Create");
}

<h2>Create</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm())
    {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Html.Translate("UserWarehouse")</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.UserProfileID, Html.Translate("UserProfile"))
        </div>
        <div class="editor-field">
            @Html.DropDownList("UserProfileID")
            @Html.ValidationMessageFor(model => model.UserProfileID)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.WarehouseID, Html.Translate("Warehouse"))
        </div>
        <div class="editor-field">
            @Html.DropDownList("WarehouseID")
            @Html.ValidationMessageFor(model => model.WarehouseID)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.IsDefault)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.IsDefault)
            @Html.ValidationMessageFor(model => model.IsDefault)
        </div>

        <p>
            <input type="submit" value="@Html.Translate("Create")" />
        </p>
    </fieldset>
    }

<div>
    @Html.ActionLink(Html.Translate("Back to List"), "Index")
</div>
